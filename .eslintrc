{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "function-paren-newline": ["error", "consistent"],
    "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }],
    "jsx-a11y/media-has-caption": [0]
  }
}
